<% layout('layouts/boilerplate') %>
<h1>Agregue nuevo lugar</h1>
<form action="/campgrounds" method="POST">
  <div>
    <label for="title">Titulo</label>
    <input
      type="text"
      placeholder="Cañon del Perreo"
      id="title"
      name="campground[title]"
    />
  </div>
  <div>
    <label for="location">Locación</label>
    <input
      type="text"
      placeholder="Tepico el Chico"
      id="location"
      name="campground[location]"
    />
  </div>
  <button>Agregar Campamento</button>
</form>
